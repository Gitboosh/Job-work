Get-MgGroup

# has to be in the format (double) property comparison (single)value(single)(double)
$itadminsGroup = Get-MgGroup -filter "DisplayName eq 'IT-Admins'"

$helpdeskGroup = Get-MgGroup -Filter "DisplayName eq 'HelpDesk'"

